{
  "name": "exocore-avs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run compile",
    "compile": "hardhat compile",
    "deploy-sepolia": "hardhat run scripts/deploy.ts --network sepolia",
    "deploy-mainnet": "hardhat run scripts/deploy.ts --network mainnet",
    "flatten": "hardhat flatten",
    "clean": "hardhat clean",
    "test": "hardhat test",
    "lint:js": "eslint . --fix",
    "lint:sol": "solhint contracts/*.sol",
    "lint": "npm run prettier && npm run lint:js && npm run lint:sol",
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "coverage": "npm run build && hardhat coverage --temp artifacts --network coverage"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^5.0.1",
    "@openzeppelin/contracts-upgradeable": "^5.0.1",
    "@openzeppelin/hardhat-upgrades": "^1.8.2",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "hardhat-gas-reporter": "^1.0.4",
    "husky-v4": "^4.3.8",
    "keccak256": "^1.0.6",
    "lint-staged": "^11.0.0",
    "merkletreejs": "^0.2.27",
    "prettier-plugin-solidity": "^1.0.0-beta.11",
    "solhint": "^3.3.6"
  },
  "dependencies": {
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^7.0.0",
    "@typechain/hardhat": "^2.0.1",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "add": "^2.0.6",
    "bignumber.js": "^9.1.1",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.3.0",
    "hardhat": "^2.4.3",
    "hardhat-deploy": "^0.7.10",
    "solidity-coverage": "^0.7.16",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.0.0",
    "typechain": "^5.0.0",
    "typescript": "^4.3.2",
    "yarn": "^1.22.21"
  }
}
